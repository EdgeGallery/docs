#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:3
msgid "The AppStore database uses an open source PostgreSQL database, and the recommended version is 12.2. The following database tables are currently designed and applied:"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:5
msgid "app_table: Store app information, such as name, provider, description, etc."
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:32
msgid "catalog_package_table: Store application package information, one application can have multiple packages"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:69
msgid "csar_package_score: Store application comments information"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:83
msgid "app_store_table: Store external appstores information"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:101
msgid "pushable_package_table: Store information about pushable application packages"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:113
msgid "message_table: Store messages during intercommunication between appstores"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:141
msgid "tbl_service_host: Store sandbox environment information"
msgstr ""

#: ../../Projects/APPSTORE/AppStore_DataBase_Design_en.md:163
msgid "tbl_uploaded_file: Store the k8s file information of the sandbox environment"
msgstr ""

